<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="AHCMS.Model" namespace="AHCMS.Models">
  <class name="Content" table="ahcms_systemprofile">
    <id name="Id">
      <generator class="guid.comb" />
    </id>
    <property name="ProfileKey" not-null="true" />
    <property name="ProfileValue">
      <column name="ProfileValue" sql-type="text"/>
    </property>
  </class>
</hibernate-mapping>
