<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="AHCMS.Model" namespace="AHCMS.Models">
  <class name="User" table="ahcms_user">
    <id name="UserId">
      <generator class="native" />
    </id>
    <property name="UserName" not-null="true" unique="true" />
    <one-to-one name="MemberShip" class="MemberShip"/>
  </class>
</hibernate-mapping>